{
    "name": "kodus-service-ast",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "type": "module",
    "main": "dist/main.js",
    "exports": {
        ".": "./dist/main.js"
    },
    "scripts": {
        "start:dev": "concurrently \"nest start --watch --debug 0.0.0.0:9231 -b swc\" \"chokidar 'dist/**/*.js' -c 'tsc-alias -p tsconfig.json'\"",
        "start:debug": "nest start --debug=0.0.0.0:9229 --watch -b swc",
        "start:debug:worker": "tsx watch --clear-screen=false --inspect=0.0.0.0:9230 src/worker/main.ts",
        "start:debug:main": "nest start --debug=0.0.0.0:9231 --watch -b swc",
        "start:debug:both": "concurrently \"yarn start:debug:main\" \"yarn start:debug:worker\"",
        "build": "nest build -b swc --type-check && tsc-alias -p tsconfig.json",
        "start:prod": "node dist/main.js",
        "start:worker": "node dist/worker/main.js",
        "start:worker:dev": "tsx watch --clear-screen=false --inspect=0.0.0.0:9230 src/worker/main.ts",
        "lint": "eslint \"src/**/*.{ts,tsx}\" \"test/**/*.{ts,tsx}\"",
        "lint:fix": "eslint \"src/**/*.{ts,tsx}\" \"test/**/*.{ts,tsx}\" --fix",
        "format:fix": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\" \"test/**/*.{ts,tsx,js,jsx}\"",
        "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md,mdx}\" \"test/**/*.{ts,tsx,js,jsx,json,md,mdx}\" \"*.{js,ts,json,md}\"",
        "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md,mdx}\" \"test/**/*.{ts,tsx,js,jsx,json,md,mdx}\" \"*.{js,ts,json,md}\" --ignore-path .prettierignore",
        "test": "jest",
        "test:nestjs": "jest test/integration/nestjs-basics.spec.ts",
        "test:messaging": "jest test/integration/rabbitmq-messaging.spec.ts",
        "health:check": "curl -s http://localhost:5001/api/health | jq",
        "health:detail": "curl -s http://localhost:5001/api/health/detail | jq",
        "docker:build": "docker compose -f docker-compose.dev.yml build",
        "docker:up": "docker compose -f docker-compose.dev.yml up",
        "docker:down": "docker compose -f docker-compose.dev.yml down --remove-orphans",
        "docker:start": "yarn docker:down && yarn docker:up",
        "docker:watch": "yarn docker:down && yarn docker:up -d",
        "docker:test": "docker compose -f docker-compose.dev.yml exec kodus-service-ast node --inspect=0.0.0.0:9301 node_modules/.bin/jest --watchAll --runInBand"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.914.0",
        "@aws-sdk/s3-request-presigner": "^3.914.0",
        "@golevelup/nestjs-rabbitmq": "^6.0.2",
        "@kodus/kodus-common": "^1.2.16",
        "@langchain/anthropic": "^0.3.31",
        "@langchain/cohere": "^0.3.4",
        "@langchain/community": "^0.3.57",
        "@langchain/core": "^0.3.78",
        "@langchain/google-genai": "^0.2.18",
        "@langchain/google-vertexai": "^0.2.18",
        "@langchain/mongodb": "^0.1.1",
        "@langchain/openai": "^0.6.16",
        "@nestjs/common": "^11.1.6",
        "@nestjs/core": "^11.1.6",
        "@nestjs/platform-express": "^11.1.6",
        "@nestjs/schedule": "^6.0.1",
        "@nestjs/terminus": "^11.0.0",
        "@testcontainers/postgresql": "^11.7.1",
        "amqp-connection-manager": "^5.0.0",
        "amqplib": "^0.10.9",
        "diff": "^8.0.2",
        "fast-xml-parser": "^5.3.0",
        "minimatch": "^10.0.3",
        "nanoid": "^5.1.6",
        "nestjs-pino": "^4.4.1",
        "pg": "^8.16.3",
        "pino": "^10.0.0",
        "pino-http": "^11.0.0",
        "pino-pretty": "^13.1.2",
        "piscina": "^5.1.3",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.2",
        "simple-git": "^3.28.0",
        "toml": "^3.0.0",
        "tree-sitter": "^0.25.0",
        "tree-sitter-c-sharp": "^0.23.1",
        "tree-sitter-go": "^0.25.0",
        "tree-sitter-java": "^0.23.5",
        "tree-sitter-javascript": "^0.25.0",
        "tree-sitter-php": "^0.24.2",
        "tree-sitter-python": "^0.25.0",
        "tree-sitter-ruby": "^0.23.1",
        "tree-sitter-rust": "^0.24.0",
        "tree-sitter-typescript": "^0.23.2",
        "ts-proto": "^2.7.7",
        "uuid": "^13.0.0"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.37.0",
        "@nestjs/cli": "^11.0.10",
        "@nestjs/schematics": "^11.0.9",
        "@nestjs/testing": "^11.1.6",
        "@swc/cli": "^0.7.8",
        "@swc/core": "^1.13.5",
        "@swc/plugin-transform-imports": "^9.1.0",
        "@testcontainers/rabbitmq": "^11.7.1",
        "@types/amqplib": "^0.10.7",
        "@types/express": "^5.0.3",
        "@types/jest": "^30.0.0",
        "@types/node": "^24.7.2",
        "@types/supertest": "^6.0.3",
        "chokidar-cli": "^3.0.0",
        "concurrently": "^9.2.1",
        "dotenv": "^17.2.3",
        "eslint": "^9.37.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "globals": "^16.4.0",
        "jest": "^30.2.0",
        "nodemon": "^3.1.10",
        "prettier": "^3.6.2",
        "source-map-support": "^0.5.21",
        "supertest": "^7.1.4",
        "testcontainers": "^11.7.1",
        "ts-jest": "^29.4.5",
        "ts-loader": "^9.5.4",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.16",
        "tsconfig-paths": "^4.2.0",
        "tsx": "^4.20.6",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.46.1"
    }
}
