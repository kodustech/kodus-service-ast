    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch --debug=0.0.0.0:9300",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "health:check": "curl -s http://localhost:5001/api/health | jq",
        "health:detail": "curl -s http://localhost:5001/api/health/detail | jq",
        "docker:build": "docker compose -f docker-compose.dev.yml build",
        "docker:up": "docker compose -f docker-compose.dev.yml up",
        "docker:down": "docker compose -f docker-compose.dev.yml down --remove-orphans",
        "docker:start": "yarn docker:down && yarn docker:up",
        "docker:watch": "yarn docker:down && yarn docker:up -d",
        "docker:test": "docker compose -f docker-compose.dev.yml exec kodus-service-ast node --inspect=0.0.0.0:9301 node_modules/.bin/jest --watchAll --runInBand"
    },
